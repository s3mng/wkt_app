{
  "expo": {
    "name": "26waffha",
    "slug": "26waffha",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "scheme": "26waffha",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0a0a0a"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.26waffha.app",
      "infoPlist": {
        "NSCameraUsageDescription": "카메라로 사물을 인식합니다.",
        "NSLocationWhenInUseUsageDescription": "지도에 현재 위치를 표시합니다.",
        "NSLocationAlwaysUsageDescription": "백그라운드에서도 위치를 추적합니다."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0a0a0a"
      },
      "package": "com.waffha.app",
      "edgeToEdgeEnabled": true,
      "permissions": [
        "CAMERA",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "FOREGROUND_SERVICE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "카메라로 사물을 인식합니다."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "지도에 현재 위치를 표시합니다."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}